{
  "name": "project-launcher",
  "appName":"Project Launcher",
  "copyright": "Copyright (c) 2023 michal.jamry@gmail.com",
  "author": "michal.jamry@gmail.com",
  "description": "project launcher and dashboard",
  "version": "1.1.0",
  "private": true,
  "main": "build/electron/electron.js",
  "homepage": "./",
  "build": {
    "extends": null,
    "files": [
      "build/**/*"
    ],
    "extraFiles": [
      "config"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mjamry/project-launcher.git"
  },
  "dependencies": {
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@mui/icons-material": "^5.8.0",
    "@mui/material": "^5.8.1",
    "@mui/utils": "^5.10.9",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.2.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/electron-devtools-installer": "^2.2.2",
    "@types/jest": "^27.5.1",
    "electron-devtools-installer": "^3.2.0",
    "electron-fetch": "^1.7.4",
    "electron-is-dev": "^2.0.0",
    "electron-reload": "^2.0.0-alpha.1",
    "esm": "^3.2.25",
    "notistack": "^3.0.1",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-router": "^6.3.0",
    "react-router-dom": "^6.3.0",
    "react-scripts": "5.0.1",
    "recoil": "^0.7.6",
    "typescript": "^4.6.4",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@mui/types": "^7.1.0",
    "@types/node": "^17.0.35",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.5",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^5.0.0",
    "concurrently": "^7.2.1",
    "cross-env": "^7.0.3",
    "electron": "^19.0.1",
    "electron-builder": "^23.6.0",
    "eslint": "^8.2.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "react-app-rewired": "^2.2.1",
    "simple-git-hooks": "^2.9.0",
    "wait-on": "^6.0.1"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build && tsc -p electron",
    "test": "react-app-rewired test",
    "lint": "eslint . --ext .tsx",
    "lint-fix": "eslint . --ext .tsx --fix",
    "electron:dev": "concurrently \"cross-env BROWSER=none npm run start\" \"wait-on http://localhost:3000 && tsc -p electron -w\" \"wait-on http://localhost:3000 && tsc -p electron && electron .\"",
    "postinstall": "electron-builder install-app-deps",
    "build:win": "electron-builder --windows --publish=never",
    "release:win": "electron-builder --windows --publish always"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "simple-git-hooks": {
    "pre-push": "npm run build"
  }
}
